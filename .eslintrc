{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn",
    "linebreak-style": "off",
    "import/extensions": "off",
    "import/no-cycle": "off",
    "no-console": "off",
    "radix": "warn",
    "no-underscore-dangle": "off"
  },

  "env": {
    "browser": true,
    "node": true
  }
}
